{
  "name": "@rapid/shared",
  "version": "0.0.1",
  "description": "Shared TypeScript utilities and types for Rapid",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./redis": {
      "types": "./dist/redis/index.d.ts",
      "import": "./dist/redis/index.js"
    },
    "./licenses.json": "./dist/licenses.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && pnpm generateLicenses",
    "clean": "rm -rf dist",
    "generateLicenses": "tsx scripts/generate-licenses.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "shared",
    "utils",
    "types",
    "typescript"
  ],
  "author": "",
  "license": "Unlicensed",
  "dependencies": {
    "redis": "5.10.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "4.0.16",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  }
}
