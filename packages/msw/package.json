{
  "name": "@tearleads/msw",
  "version": "0.0.8",
  "description": "Shared MSW handlers for Tearleads tests",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "cross-env DOTENV_CONFIG_QUIET=true vitest run",
    "test:coverage": "cross-env DOTENV_CONFIG_QUIET=true vitest run --coverage"
  },
  "keywords": [
    "msw",
    "testing",
    "mocks"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@tearleads/shared": "workspace:*",
    "msw": "2.12.10"
  },
  "devDependencies": {
    "@types/node": "25.3.0",
    "@vitest/coverage-v8": "4.0.18",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  }
}
