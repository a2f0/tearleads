{
  "name": "@tearleads/app-builder",
  "version": "1.0.2",
  "private": true,
  "description": "White-label app builder for multi-app builds",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./types": {
      "import": "./src/types.ts",
      "types": "./src/types.ts"
    }
  },
  "bin": {
    "app-builder": "./src/cli.ts"
  },
  "scripts": {
    "generate": "tsx src/cli.ts generate",
    "validate": "tsx src/cli.ts validate",
    "list": "tsx src/cli.ts list",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tearleads/shared": "workspace:*",
    "commander": "14.0.3",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@types/node": "25.2.3",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  }
}
