{
  "name": "@tearleads/tee-api",
  "version": "0.0.4",
  "description": "Secure API primitives and reference server for the TEE environment",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "buildBundle": "esbuild src/server.ts --bundle --platform=node --target=node24 --format=cjs --outfile=dist/server.cjs --sourcemap",
    "clean": "rm -rf dist",
    "dev": "tsx watch --clear-screen=false src/server.ts",
    "test": "cross-env DOTENV_CONFIG_QUIET=true vitest run",
    "test:coverage": "cross-env DOTENV_CONFIG_QUIET=true vitest run --coverage"
  },
  "keywords": [
    "tee",
    "api",
    "security",
    "privacy"
  ],
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/node": "25.3.0",
    "@vitest/coverage-v8": "4.0.18",
    "cross-env": "10.1.0",
    "esbuild": "0.27.3",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  }
}
