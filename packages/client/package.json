{
  "name": "@tearleads/client",
  "version": "0.0.782",
  "description": "React client application for Tearleads",
  "main": "out/main/main.js",
  "type": "module",
  "scripts": {
    "dev": "tsx ../../scripts/killPnpmDev.ts && cross-env DOTENV_CONFIG_QUIET=true vite",
    "build": "tsc && cross-env DOTENV_CONFIG_QUIET=true vite build",
    "preview": "cross-env DOTENV_CONFIG_QUIET=true vite preview",
    "clean": "rm -rf dist",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "cap:sync": "cap sync",
    "cap:open:android": "cap open android",
    "cap:run:android": "cap run android",
    "build:android": "pnpm build && cap sync android",
    "cap:open:ios": "cap open ios",
    "cap:run:ios": "cap run ios",
    "build:ios": "pnpm build && cap sync ios",
    "lint:scripts": "shellcheck --severity=info scripts/*.sh",
    "lint:rubocop": "bundle exec rubocop",
    "test:maestro:android": "../../scripts/runMaestroAndroidTests.sh",
    "test:maestro:ios": "../../scripts/runMaestroIosTests.sh",
    "test": "cross-env DOTENV_CONFIG_QUIET=true vitest run",
    "test:coverage": "cross-env DOTENV_CONFIG_QUIET=true vitest run --coverage",
    "test:watch": "cross-env DOTENV_CONFIG_QUIET=true vitest",
    "electron:prepare-sqlite": "../../scripts/prepareElectronSqliteBinary.sh",
    "electron:dev": "pnpm electron:prepare-sqlite && cross-env DOTENV_CONFIG_QUIET=true electron-vite dev",
    "electron:build": "pnpm electron:prepare-sqlite && cross-env DOTENV_CONFIG_QUIET=true electron-vite build",
    "electron:preview": "pnpm electron:prepare-sqlite && cross-env DOTENV_CONFIG_QUIET=true electron-vite preview",
    "electron:pack": "pnpm electron:build && electron-builder --config electron-builder.config.ts",
    "electron:dist": "pnpm electron:build && electron-builder --config electron-builder.config.ts --publish=never",
    "electron:dist:unsigned": "pnpm electron:build && cross-env CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --config electron-builder.config.ts --publish=never",
    "electron:test": "pnpm electron:build && playwright test --config=playwright.electron.config.ts",
    "electron:test:headed": "pnpm electron:build && HEADED=true playwright test --config=playwright.electron.config.ts",
    "electron:test:ui": "pnpm electron:build && playwright test --config=playwright.electron.config.ts --ui"
  },
