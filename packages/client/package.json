{
  "name": "@rapid/client",
  "version": "0.0.156",
  "description": "React client application for Rapid",
  "main": "out/main/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "cap:sync": "cap sync",
    "cap:open:android": "cap open android",
    "cap:run:android": "cap run android",
    "build:android": "pnpm build && cap sync android",
    "cap:open:ios": "cap open ios",
    "cap:run:ios": "cap run ios",
    "build:ios": "pnpm build && cap sync ios",
    "lint:scripts": "shellcheck --severity=info scripts/*.sh",
    "lint:rubocop": "bundle exec rubocop",
    "test:maestro:android": "../../scripts/runMaestroAndroidTests.sh",
    "test:maestro:ios": "../../scripts/runMaestroIosTests.sh",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "electron:dev": "electron-vite dev",
    "electron:build": "electron-vite build",
    "electron:preview": "electron-vite preview",
    "electron:pack": "electron-vite build && electron-builder --config electron-builder.config.ts",
    "electron:dist": "electron-vite build && electron-builder --config electron-builder.config.ts --publish=never",
    "electron:dist:unsigned": "CSC_IDENTITY_AUTO_DISCOVERY=false electron-vite build && electron-builder --config electron-builder.config.ts --publish=never",
    "electron:test": "pnpm electron:build && playwright test --config=playwright.electron.config.ts",
    "electron:test:headed": "pnpm electron:build && HEADED=true playwright test --config=playwright.electron.config.ts",
    "electron:test:ui": "pnpm electron:build && playwright test --config=playwright.electron.config.ts --ui",
    "postinstall": "electron-rebuild -f -o better-sqlite3-multiple-ciphers"
  },
  "dependencies": {
    "@assistant-ui/react": "0.11.53",
    "@capacitor-community/sqlite": "7.0.2",
    "@capacitor/app": "8.0.0",
    "@capacitor/core": "8.0.0",
    "@capacitor/filesystem": "8.0.0",
    "@capacitor/preferences": "8.0.0",
    "@capacitor/share": "8.0.0",
    "@capawesome/capacitor-file-picker": "8.0.0",
    "@capgo/capacitor-native-biometric": "8.0.3",
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/sortable": "10.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@huggingface/transformers": "3.4.2",
    "@ionic/core": "8.7.16",
    "@radix-ui/react-slot": "1.2.4",
    "@rapid/db": "workspace:*",
    "@rapid/shared": "workspace:*",
    "@rapid/ui": "workspace:*",
    "@tanstack/react-virtual": "3.13.18",
    "class-variance-authority": "0.7.1",
    "drizzle-orm": "0.45.1",
    "file-type": "21.2.0",
    "i18next": "25.7.4",
    "i18next-browser-languagedetector": "8.2.0",
    "invariant": "2.2.4",
    "lucide-react": "0.562.0",
    "music-metadata": "11.10.5",
    "pdfjs-dist": "5.4.296",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-i18next": "16.5.2",
    "react-pdf": "10.3.0",
    "react-router-dom": "7.11.0",
    "recharts": "3.6.0",
    "sonner": "2.0.7"
  },
  "devDependencies": {
    "@capacitor/android": "8.0.0",
    "@capacitor/cli": "8.0.0",
    "@capacitor/ios": "8.0.0",
    "@electron-toolkit/preload": "3.0.2",
    "@electron-toolkit/utils": "4.0.0",
    "@electron/rebuild": "4.0.2",
    "@playwright/test": "1.57.0",
    "@tailwindcss/vite": "4.1.18",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@testing-library/user-event": "14.6.1",
    "@types/better-sqlite3": "7.6.13",
    "@types/invariant": "2.2.37",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/wicg-file-system-access": "2023.10.7",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.16",
    "@webgpu/types": "0.1.68",
    "better-sqlite3-multiple-ciphers": "12.5.0",
    "drizzle-kit": "0.31.8",
    "electron": "39.2.7",
    "electron-builder": "26.0.12",
    "electron-vite": "5.0.0",
    "jsdom": "27.4.0",
    "msw": "2.6.5",
    "svgo": "4.0.0",
    "tailwindcss": "4.1.18",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vite-plugin-pwa": "1.2.0",
    "vitest": "4.0.16"
  }
}
