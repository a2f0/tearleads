{
  "name": "@tearleads/api-client",
  "version": "0.0.1",
  "description": "Shared API consumer client for Tearleads",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./authStorage": {
      "types": "./src/authStorage.ts",
      "import": "./src/authStorage.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "echo \"@tearleads/api-client is source-consumed via Vite aliases\"",
    "clean": "rm -rf dist",
    "test": "cross-env DOTENV_CONFIG_QUIET=true vitest run",
    "test:coverage": "cross-env DOTENV_CONFIG_QUIET=true vitest run --coverage"
  },
  "dependencies": {
    "@tearleads/shared": "workspace:*"
  },
  "devDependencies": {
    "@tearleads/msw": "workspace:*",
    "@types/node": "25.2.3",
    "@vitest/coverage-v8": "4.0.18",
    "cross-env": "10.1.0",
    "jsdom": "28.1.0",
    "typescript": "5.9.3",
    "vitest": "4.0.18",
    "vitest-fail-on-console": "0.10.1"
  }
}
