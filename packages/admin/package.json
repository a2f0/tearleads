{
  "name": "@tearleads/admin",
  "version": "0.0.34",
  "description": "Admin pages and windows for Tearleads",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./clientEntry": {
      "types": "./src/clientEntry.ts",
      "import": "./src/clientEntry.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "echo \"@tearleads/admin is source-consumed via Vite aliases\"",
    "clean": "rm -rf dist",
    "test": "cross-env DOTENV_CONFIG_QUIET=true vitest run",
    "test:coverage": "cross-env DOTENV_CONFIG_QUIET=true vitest run --coverage"
  },
  "peerDependencies": {
    "@tearleads/ui": "workspace:*",
    "@tearleads/window-manager": "workspace:*",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-router-dom": "7.13.1"
  },
  "dependencies": {
    "@tanstack/react-virtual": "3.13.19",
    "@tearleads/backups": "workspace:*",
    "@tearleads/compliance": "workspace:*",
    "@tearleads/shared": "workspace:*",
    "lucide-react": "0.575.0",
    "react-i18next": "16.5.4",
    "sonner": "2.0.7"
  },
  "devDependencies": {
    "@tearleads/app-builder": "workspace:*",
    "@tearleads/ui": "workspace:*",
    "@tearleads/window-manager": "workspace:*",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "25.3.2",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.4",
    "@vitest/coverage-v8": "4.0.18",
    "cross-env": "10.1.0",
    "i18next": "25.8.13",
    "jsdom": "28.1.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-router-dom": "7.13.1",
    "typescript": "5.9.3",
    "vitest": "4.0.18",
    "vitest-fail-on-console": "0.10.1"
  }
}
