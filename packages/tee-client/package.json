{
  "name": "@tearleads/tee-client",
  "version": "0.0.1",
  "description": "TEE API client with request/response security and privacy assertions",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "cross-env DOTENV_CONFIG_QUIET=true vitest run",
    "test:coverage": "cross-env DOTENV_CONFIG_QUIET=true vitest run --coverage"
  },
  "keywords": [
    "tee",
    "client",
    "security",
    "privacy"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@tearleads/tee-api": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "25.2.3",
    "@vitest/coverage-v8": "4.0.18",
    "cross-env": "10.1.0",
    "msw": "2.12.10",
    "typescript": "5.9.3",
    "vitest": "4.0.18",
    "vitest-fail-on-console": "0.10.1"
  }
}
